{
  "name": "server-v2",
  "version": "1.2.0",
  "description": "Server for ammarahmed.ca written in Typescript with GraphQL API, MongoDB database with data from Notion. ",
  "main": "./dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@ammar-ahmed22/chess-engine": "^0.4.1",
    "@ammarahmedca/types": "^0.0.13",
    "@apollo/server": "^4.1.1",
    "@graphql-tools/schema": "^9.0.9",
    "@notionhq/client": "^2.2.2",
    "@typegoose/typegoose": "^9.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.2.2",
    "@types/nodemailer": "^6.4.6",
    "apollo-server-express": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "graphql": "^16.0.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.4",
    "jest": "^29.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.3",
    "nodemailer": "^6.8.0",
    "notion-client": "^6.15.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsc": "^2.0.4",
    "type-graphql": "^2.0.0-beta.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "babel-jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "start:dev": "nodemon --watch './**/*.ts' --exec ts-node ./src/index.ts",
    "test": "jest",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.json src/**/*.html dist/",
    "build": "yarn clean && yarn copy-files && tsc",
    "start": "node ./dist/index.js",
    "build:start": "yarn build && yarn start",
    "publish-schema": "rover graph publish ammarahmedca-api-v2@production --schema ./src/schema.gql",
    "format": "prettier --write .",
    "emit-schema": "EMIT_SCHEMA=true node ./dist/index.js"
  }
}
